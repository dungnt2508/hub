{
    "name": "bot_v4-root",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "frontend:dev": "cd frontend && npm run dev",
        "frontend:build": "cd frontend && npm run build",
        "backend:dev": "python -m uvicorn app.main:app --reload --port 8386",
        "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
        "install:all": "npm install && cd frontend && npm install && pip install -r requirements.txt"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}